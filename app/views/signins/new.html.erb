<h1>Sign In Page</h1>
<img src= "https://i.ibb.co/Z2jxnvM/santa.jpg" width="250" height="230">
<style>
body {
  background-color: #C3FDB8;
  
}

</style>
<%= form_for :Welcome!, url: signins_path do |f| %>
  <p>
    <%= f.label :Username %><br>
    <%= f.text_field :user %>
    <br>
    <%= f.label :Password %><br>
   
   <%= f.password_field :pass %>

    
  </p>
 

 
  <p>
    <%= f.submit %>
  </p>
<% end %>
