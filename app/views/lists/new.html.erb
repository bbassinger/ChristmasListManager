<h1>List Page!</h1>
<style>
body {
  background-image: url("https://i.ibb.co/bzCSMYG/santatree.png");
}
</style>
<%= form_for :article do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
