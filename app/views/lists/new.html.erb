<h1>Add to your Christmas List!</h1>
<%= link_to 'My List', controller: 'lists' %>
<%= form_for :list, url: lists_path do |f| %>
  <p>
    <%= f.label :Product %><br>
    <%= f.text_field :product %>
  </p>
 
  <p>
    <%= f.label :Store %><br>
    <%= f.text_area :store %>
  </p>
  
  <p>
    <%= f.label :Price %><br>
   <%= f.select(:price, [['Under $5.00'], ['$5.00-$20.00'],['Above $20.00']])%>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', lists_path %><br>
<%= link_to 'Log Out', logout_path %>



