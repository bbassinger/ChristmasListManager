<h1>Your Christmas Lists!</h1>
<%= link_to 'My List', controller: 'lists' %>
<%= form_for :list, url: lists_path do |f| %>
  <p>
    <%= f.label :Product %><br>
    <%= f.text_field :product %>
  </p>
 
  <p>
    <%= f.label :Store %><br>
    <%= f.text_area :store %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
